/* pages/my/my.wxss */
.page {
  width: 100%;
  height: 100%;
}
.my-info {
  margin: 2%;
  color: #919191;
  font-size: 13px;
  background-color: #fff;
}
.my-image {
  margin: 2%;
  display: flex;
  align-items: center;
}
.userinfo-avatar {
  width: 50px;
  height: 50px;
  border-radius: 50%;
}
.function {
  background-color: #fff;
  margin: 5%;
  font-size: 13px;
}
.custom-appo {
  padding-top: 2%;
  padding-bottom: 2%;
  border-bottom: 1px solid #ccc;
  display: flex;
  align-items: center;
}
.function-img {
  width: 43rpx;
  height: 43rpx;
  margin: 5rpx;
  margin-right: 5rpx;
}
